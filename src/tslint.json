{
  "extends": "tslint:recommended",
  "rules": {
      "max-line-length": {
          "options": [120]
      },
      "trailing-comma": [true, {"multiline": "always", "singleline": "never"}],
      "object-literal-sort-keys": false,
      "no-console": false,
      "no-empty": false,
      "linebreak-style": [true, "CRLF"],
      "max-classes-per-file": false,
      "whitespace": [true, "check-module"]
  }
}